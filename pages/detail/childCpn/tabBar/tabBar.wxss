/* pages/detail/childCpn/tabBar/tabBar.wxss */
.tabBar{
  width: 100%;
  height: 120rpx;
  display: flex;
  flex-direction: row;
  position: fixed;
  bottom:0;
  left: 0;
  right: 0;
  background-color: #eee;
  font-size: 30rpx;
}

.icons{
  flex:5;
  display: flex;
  flex-direction: row;
}
.iconsItem{
  flex:1;
  text-align: center;
  margin:14rpx;
}
.iconsItem image{
  width: 50rpx;
  height: 50rpx;
}

.btns{
  height: 120rpx;
  line-height: 120rpx;
  flex:5;
  display: flex;
  flex-direction: row;
}
.btns .left{
  flex: 6;
  background-color: yellow;
  color: #000;
  text-align: center;
}
.btns .right{
  flex: 5;
  background-color: #ff5777;
  color: #fff;
  text-align: center;
}